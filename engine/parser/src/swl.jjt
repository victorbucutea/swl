// Layout Components

| < VERTICAL_LAYOUT: "vertical_layout" >
| < HORIZONTAL_LAYOUT: "horizontal_layout" >
| < HORIZONTAL_FORM: "horizontal_form" >
| < HORIZONTAL_FORM_GROUP: "horizontal_form_group" >
| < SECTION: "section" >


// HTML components
| < BR: "br" >
